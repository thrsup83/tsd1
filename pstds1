select SMS_R_System.ResourceId, SMS_R_System.ResourceType, SMS_R_System.Name, 
SMS_R_System.SMSUniqueIdentifier, SMS_R_System.ResourceDomainORWorkgroup, SMS_R_System.Client 
from SMS_R_System 
inner join SMS_G_System_ConfigPolicyComplianceState on 
SMS_G_System_ConfigPolicyComplianceState.ResourceID = SMS_R_System.ResourceId 
where SMS_G_System_ConfigPolicyComplianceState.ComplianceState = 2 
and SMS_G_System_ConfigPolicyComplianceState.ConfigPolicyName = "NOME_DA_SUA_BASELINE"

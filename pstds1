$base = Join-Path $env:windir 'System32\GroupPolicy\Machine\Microsoft\Windows NT'
$gt   = Join-Path $base 'SecEdit\GptTmpl.inf'

"BASE EXISTS = $(Test-Path $base)  ->  $base"
"GPTTMPL EXISTS = $(Test-Path $gt)  ->  $gt"

if (Test-Path $gt) {
  Select-String -Path $gt -Pattern '^SeBatchLogonRight'
} else {
  "Sem SecEdit/GptTmpl. Subpastas em '$base':"
  Get-ChildItem $base -Force -ErrorAction SilentlyContinue | Select Name, LastWriteTime
}

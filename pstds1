$p = "SYSTEM\CurrentControlSet\Control\Session Manager\Memory Management"
$reg = [Microsoft.Win32.Registry]::LocalMachine
$k = $reg.OpenSubKey($p)

$vm = if($k){$k.GetValue("FeatureSettingsOverrideMask")}else{$null}
$vo = if($k){$k.GetValue("FeatureSettingsOverride")}else{$null}

$r1 = if($null -eq $vm){"Null"}else{"0x$("{0:X}" -f [int]$vm) ($vm)"}
$r2 = if($null -eq $vo){"Null"}else{"0x$("{0:X}" -f [int]$vo) ($vo)"}

"M:$r1 | O:$r2"

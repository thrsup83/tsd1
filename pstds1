$db  = Join-Path $env:windir 'security\Database\secedit.sdb'
$log = Join-Path $env:windir 'security\logs\scesrv.log'

Get-Item $db,$log -ErrorAction SilentlyContinue |
  Select FullName, LastWriteTime, Length

if (Test-Path $log) {
  "`n--- scesrv.log (ultimas linhas que citam rights/template) ---"
  Select-String -Path $log -SimpleMatch -Pattern `
    'SeBatchLogonRight','SeServiceLogonRight','secedit','template','.inf' |
    Select-Object -Last 30
} else {
  "scesrv.log nao encontrado"
}

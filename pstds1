# 1. Mata todos os processos do SCCM que podem estar travados
taskkill /F /IM ccmsetup.exe /T /EA 0
taskkill /F /IM ccmexec.exe /T /EA 0

# 2. Deleta o serviço do Windows na marra (o culpado do erro de reinstalação)
sc.exe delete CcmExec
sc.exe delete ccmsetup

# 3. Limpa as pastas de instalação de novo
Remove-Item "C:\Windows\CCM", "C:\Windows\ccmsetup", "C:\Windows\ccmcache" -Recurse -Force -EA 0

select SMS_R_System.ResourceId, SMS_R_System.ResourceType, SMS_R_System.Name, SMS_R_System.SMSUniqueIdentifier, SMS_R_System.ResourceDomainORWorkgroup, SMS_R_System.Client 
from SMS_R_System 
where SMS_R_System.ResourceId in (
    select ResourceId from SMS_G_System_ADD_REMOVE_PROGRAMS 
    where DisplayName like "Notepad++%" and Version != "8.9.1"
) 
or SMS_R_System.ResourceId in (
    select ResourceId from SMS_G_System_ADD_REMOVE_PROGRAMS_64 
    where DisplayName like "Notepad++%" and Version != "8.9.1"
)

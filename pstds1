$Proc = Get-CimInstance Win32_Processor
$HTStatus = if ($Proc.NumberOfLogicalProcessors -gt $Proc.NumberOfCores) { "Enabled" } else { "Disabled" }

return [PSCustomObject]@{
    CPU_Model  = $Proc.Name
    Physical   = $Proc.NumberOfCores
    Logical    = $Proc.NumberOfLogicalProcessors
    HT_Status  = $HTStatus
}
